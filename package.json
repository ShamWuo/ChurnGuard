{
	"name": "dunnr",
	"version": "1.0.2",
	"private": false,
	"description": "Dunnr â€“ Stripe churn deflection: automated dunning, recovery analytics, admin console.",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/ShamWuo/ChurnGuard.git"
	},
	"homepage": "https://github.com/ShamWuo/ChurnGuard#readme",
	"scripts": {
		"dev": "next dev",
		"dev:3100": "next dev -p 3100",
		"dev:3100:env": "powershell -NoProfile -Command \"$env:ADMIN_SECRET='secret123'; $env:CSRF_SECRET='csrf123'; $env:NEXT_TELEMETRY_DISABLED='1'; npx next dev -p 3100\"",
		"dev:win": "powershell -NoProfile -File ./scripts/start-dev-win.ps1 -Port 3100 -AdminSecret secret123 -CsrfSecret csrf123",
		"demo": "node scripts/seed-demo.js && npm run dev:3100:env",
		"demo:win": "node scripts/seed-demo.js && npm run dev:win",
		"seed:demo": "node scripts/seed-demo.js",
		"build": "next build",
		"build:clean": "rimraf .next && next build",
		"start": "next start",
		"clean": "rimraf .next",
		"prisma:generate": "prisma generate",
		"prisma:push": "prisma db push",
		"prisma:deploy": "prisma migrate deploy",
		"prisma:generate:pg": "prisma generate --schema=prisma/schema.postgres.prisma",
		"prisma:migrate:pg": "prisma migrate dev --name init_enums --schema=prisma/schema.postgres.prisma",
		"prisma:deploy:pg": "prisma migrate deploy --schema=prisma/schema.postgres.prisma",
		"test": "jest --runInBand",
		"test:recovered": "jest __tests__/recovered.test.ts --runInBand --verbose",
		"smoke:ready": "node scripts/smoke-ready.js",
		"precompile-templates": "node scripts/precompile-templates.js",
		"ci": "npm run precompile-templates && npm test",
		"release:patch": "npm version patch -m \"chore(release): %s\" && git push --follow-tags",
		"docker:prod": "docker build -t dunnr:prod .",
		"e2e": "npm run build:clean && playwright test",
		"e2e:run": "playwright test",
		"e2e:dev": "cross-env E2E_USE_DEV=true npx playwright test",
		"e2e:dev:win": "powershell -NoProfile -File ./scripts/run-e2e-dev.ps1 -Port 3100",
		"e2e:headed": "npm run build:clean && playwright test --headed",
		"e2e:start": "npm run build:clean && npm run start",
		"backfill:recovery": "node scripts/backfill-recovery.js"
	},
	"dependencies": {
		"@prisma/client": "^5.22.0",
		"@sentry/node": "^10.5.0",
		"html-to-text": "^9.0.0",
		"ioredis": "^5.7.0",
		"jsonwebtoken": "^9.0.2",
		"next": "14.2.5",
		"nodemailer": "^6.9.13",
		"postmark": "^4.0.5",
		"raw-body": "^2.5.2",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"stripe": "^14.23.0",
		"swr": "^2.2.5",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@playwright/test": "^1.55.0",
		"@types/jest": "^30.0.0",
		"@types/jsonwebtoken": "^9.0.2",
		"@types/node": "^20.12.7",
		"@types/nodemailer": "^7.0.1",
		"@types/raw-body": "^2.1.4",
		"@types/react": "^18.2.74",
		"@types/react-dom": "^18.2.24",
		"cross-env": "^7.0.3",
		"handlebars": "^4.7.8",
		"jest": "^30.0.5",
		"node-fetch": "^2.6.7",
		"node-mocks-http": "^1.17.2",
		"prisma": "^5.22.0",
		"rimraf": "^5.0.5",
		"ts-jest": "^29.4.1",
		"typescript": "^5.4.5"
	}
}
